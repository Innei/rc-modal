{
  "name": "rc-modal-sheet",
  "version": "0.0.1",
  "description": "TODO",
  "author": "Innei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Innei/rc-modal"
  },
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./src/*": {
      "import": "./src/*"
    }
  },
  "publishConfig": {
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "types/index.d.ts",
    "exports": {
      ".": {
        "type": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./motion": {
        "type": "./dist/motion.d.ts",
        "import": "./dist/motion.js",
        "require": "./dist/motion.cjs"
      },
      "./m": {
        "type": "./dist/m.d.ts",
        "import": "./dist/m.js",
        "require": "./dist/m.cjs"
      },
      "./mobile-detector": {
        "type": "./dist/mobile-detector.d.ts",
        "import": "./dist/mobile-detector.js",
        "require": "./dist/mobile-detector.cjs"
      }
    },
    "files": [
      "dist",
      "lib",
      "esm",
      "readme.md",
      "tsconfig.json",
      "types",
      "src"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write ",
      "eslint --cache"
    ]
  },
  "bump": {
    "before": [
      "npm run build"
    ],
    "publish": true,
    "changelog": true
  },
  "scripts": {
    "prebuild": "rm -rf lib && rm -rf esm",
    "build": "vite build && tailwindcss -i src/tw.css -o dist/index.css",
    "prepare": "husky install",
    "release": "bump"
  },
  "devDependencies": {
    "@innei/bump-version": "1.5.10",
    "@innei/eslint-config-react-ts": "0.12.4",
    "@innei/eslint-config-ts": "0.12.4",
    "@innei/prettier": "0.12.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@types/node": "20.11.27",
    "@types/react": "18.2.65",
    "@vitejs/plugin-react": "4.2.1",
    "dts-bundle-generator": "^9.3.1",
    "esbuild": "0.20.1",
    "foxact": "0.2.33",
    "framer-motion": "11.0.13",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "postcss": "8.4.35",
    "postcss-import": "16.0.1",
    "postcss-nested": "6.0.1",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "tailwindcss": "3.4.1",
    "tsc-alias": "1.8.8",
    "tslib": "2.6.2",
    "typescript": "5.4.2",
    "vite": "5.1.6",
    "vite-plugin-dts": "3.7.3",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.3.1"
  },
  "peerDependencies": {
    "@radix-ui/react-dialog": "^1",
    "clsx": "*",
    "framer-motion": "^11",
    "tailwind-merge": "^2",
    "vaul": "^0.9.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1",
    "clsx": "*",
    "framer-motion": "^11",
    "tailwind-merge": "^2",
    "vaul": "^0.9.0"
  }
}